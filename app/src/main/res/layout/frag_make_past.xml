<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <io.dcloud.H58E83894.weiget.overscroll.FastAndOverScrollScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/border_space_sec_m"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/past_see_all"
                    android:button="@null"
                    android:layout_width="0dip"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="@string/str_corret_see_all"
                    android:gravity="center"/>
                <View
                    style="@style/lines"
                    android:layout_height="15dip"
                    android:layout_width="1dip"
                    android:layout_gravity="center_horizontal"
                    android:padding="@dimen/radius"
                    />

                <TextView
                    android:id="@+id/past_see_mine"
                    android:button="@null"
                    android:layout_width="0dip"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="@string/str_corret_see_mine"
                    android:gravity="center"/>
            </LinearLayout>

            <View
                style="@style/line"
                android:layout_height="@dimen/border_space_sec"
                />

            <FrameLayout
                android:id="@+id/frame"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                ></FrameLayout>

        </LinearLayout>

    </io.dcloud.H58E83894.weiget.overscroll.FastAndOverScrollScrollView>
</RelativeLayout>