<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <size
                android:height="2dp"/>
            <corners android:radius="2dp"/>
            <solid android:color="@color/color_sec_gray"/>
        </shape>
    </item>
    <item
        android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <size
                    android:height="2dp"/>
                <corners android:radius="2dp"/>
                <solid android:color="@color/color_sec_gray"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <size
                    android:height="2dp"/>
                <corners android:radius="2dp"/>
                <solid android:color="@color/color_read_practice"/>
            </shape>
        </clip>
    </item>
</layer-list>
